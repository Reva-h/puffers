{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome Dev Docs",
    "version": "1.0",
    "icons": {
      "64": "images/puffers_icon_64"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
    ],
    "permissions": [
        "tabs",
        "activeTab"
        ]
  }